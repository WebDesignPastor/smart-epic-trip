{
  "compilerOptions": {
    "composite": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "es2021",
      "dom"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./microservices/build",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "es2017"
  }
}
